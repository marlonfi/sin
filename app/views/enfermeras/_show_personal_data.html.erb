<div class="panel-body bio-graph-info">
    <h1>Datos de la enfermera</h1>
    <div class="row">
        <div class="bio-row">
            <p><span>Ap. Paterno </span>: <%= @enfermera.apellido_paterno%></p>
        </div>
        <div class="bio-row">
            <p><span>Ap. Materno </span>: <%= @enfermera.apellido_materno%></p>
        </div>
        <div class="bio-row">
            <p><span>Nombres </span>: <%= @enfermera.nombres %></p>
        </div>
        <div class="bio-row">
            <p><span>Sexo</span>: <%= @enfermera.sexo %></p>
        </div>
        <div class="bio-row">
            <p><span>Nacimiento </span>: <%= fecha_con_formato @enfermera.fecha_nacimiento %></p>
        </div>
        <div class="bio-row">
            <p><span>Email </span>: <%=@enfermera.email %></p>
        </div>
        <div class="bio-row">
            <p><span>DNI </span>: <%=@enfermera.dni %></p>
        </div>
        <div class="bio-row">
            <p><span>Teléfonos</span>: <%=@enfermera.telefono %></p>
        </div>        
        <div class="bio-row">
            <p><span>Domicilio</span>: <%=@enfermera.domicilio_completo %></p>
        </div>
        <div class="bio-row">
            <p><span>Factor Sanguineo </span>: <%=@enfermera.factor_sanguineo %></p>
        </div>
        <div class="bio-row">
            <p><span>Especialidad </span>: <%=@enfermera.especialidad %></p>
        </div>
        <div class="bio-row">
            <p><span>¿Maestria? </span>: <%=@enfermera.maestria %></p>
        </div>
        <div class="bio-row">
            <p><span>¿Doctorado? </span>: <%=@enfermera.doctorado %></p>
        </div>
    </div>
</div>
