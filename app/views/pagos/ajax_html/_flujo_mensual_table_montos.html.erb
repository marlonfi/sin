<div class="col-lg-12">
  <section class="panel">
    <header class="panel-heading">
      Total de aportaciones de la cotizaciones correspondientes a <%= month_year(Date.parse(@cotizacion)) %> 
    </header>
    <div class="panel-body">
        <table class="table table-striped table-advance table-hover">
          <thead>
            <tr>
              <th><i class="fa fa-h-square"></i> Cod. Base</th>
              <th><i class="fa fa-usd"></i> Ingresos por Contr. y Nom.</th>
              <th><i class="fa fa-usd"></i> Ingresos por CAS</th>
              <th><i class="fa fa-usd"></i> Ingresos VOUCHER.</th>
              <th><i class="fa fa-usd"></i> Total Ingresos</th>
              <th><i class="fa fa-usd"></i> Asignación Base</th>
              <th><i class="fa fa-usd"></i> Asignación CEN</th>
            </tr>
          </thead>            
          <tbody>
            <tr>
              <th><%= @codigo_base %></th>
              <th><%= number_to_currency(@money_contratados, :unit => "S/. ") %></th>
              <th><%= number_to_currency(@money_cas, :unit => "S/. ") %></th>
              <th><%= number_to_currency(@money_voucher, :unit => "S/. ") %></th>
              <th><%= number_to_currency(@money_total, :unit => "S/. ") %></th>
              <th><%= number_to_currency(@asignacion, :unit => "S/. ") %></th>
              <th><%= number_to_currency(@asignacion, :unit => "S/. ") %></th>
            </tr>
          </tbody>
        </table>
      </div>      
  </section>
</div>

